<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>ga.documentprocessing.service-secrets</UserSecretsId>
    <WarningsAsErrors>CS1998</WarningsAsErrors>
    <NoWarn>$(NoWarn);SKEXP0001</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aspire.StackExchange.Redis.DistributedCaching" Version="9.5.2"/>
    <PackageReference Include="Microsoft.Extensions.Caching.StackExchangeRedis" Version="9.0.10"/>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.9.0"/>
    <PackageReference Include="Swashbuckle.AspNetCore.Annotations" Version="6.9.0"/>
    <PackageReference Include="Hellang.Middleware.ProblemDetails" Version="6.5.1"/>
    <PackageReference Include="MongoDB.Driver" Version="3.2.0"/>
    <!-- PDF Processing -->
    <PackageReference Include="itext7" Version="9.0.0"/>
    <PackageReference Include="itext7.pdfhtml" Version="6.0.0"/>
    <!-- Markdown Processing -->
    <PackageReference Include="Markdig" Version="0.40.0"/>
    <!-- HTTP Client -->
    <PackageReference Include="Microsoft.Extensions.Http.Resilience" Version="9.1.0"/>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\AllProjects.ServiceDefaults\AllProjects.ServiceDefaults.csproj"/>
    <ProjectReference Include="..\..\..\Common\GA.Business.Core\GA.Business.Core.csproj"/>
    <ProjectReference Include="..\..\..\Common\GA.Core\GA.Core.csproj"/>
    <ProjectReference Include="..\..\..\GA.Data.MongoDB\GA.Data.MongoDB.csproj"/>
    <ProjectReference Include="..\..\..\GA.Data.SemanticKernel.Embeddings\GA.Data.SemanticKernel.Embeddings.csproj"/>
  </ItemGroup>
</Project>

