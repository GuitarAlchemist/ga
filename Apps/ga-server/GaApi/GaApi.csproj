    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>0c749f53-3fda-4099-a8d4-ee77ffdd1913</UserSecretsId>
    <!-- Treat async method without await as error -->
    <WarningsAsErrors>CS1998</WarningsAsErrors>

    <!-- Disable auto-generation of AssemblyInfo.cs to avoid duplicates -->
