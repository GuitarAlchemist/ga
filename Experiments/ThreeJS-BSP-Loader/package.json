{
  "name": "threejs-bsp-loader",
  "version": "1.0.0",
  "description": "Three.js BSP-style scene loader with cells/portals and BVH acceleration",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "install-deps": "npm install",
    "start": "npm run dev"
  },
  "dependencies": {
    "three": "^0.168.0",
    "three-mesh-bvh": "^0.8.0"
  },
  "devDependencies": {
    "@types/three": "^0.168.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0"
  },
  "keywords": [
    "threejs",
    "bsp",
    "webgpu",
    "portal-culling",
    "bvh",
    "3d",
    "game-engine"
